</h1>
<div class="ms-webpart-zone ms-fullWidth">
<div id="MSOZoneCell_WebPartctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe" class="s4-wpcell-plain ms-webpartzone-cell ms-webpart-cell-vertical ms-fullWidth ">
<div class="ms-webpart-chrome ms-webpart-chrome-vertical ms-webpart-chrome-fullWidth ">
<div WebPartID="53ff1dc6-e602-4006-989b-0563635783fe" HasPers="false" id="WebPartctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe" width="100%" class="ms-WPBody noindex " allowDelete="false" allowExport="false" style=""><div id="ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe">
<script type="text/javascript">

    function ceoBlogCommentsValidate() {
            

        var firstName = jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_inputfirstName').val();
        if (firstName == "") {
            jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_lblErrorCaptcha').text("Please enter a value for First Name");
            return false;
        }

        var inputlastName = jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_inputlastName').val();
        if (inputlastName == "") {
            jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_lblErrorCaptcha').text("Please enter a value for Last Name");
            return false;
        }

        var inputEmail = jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_inputEmail').val();
        if (inputEmail == "") {
            jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_lblErrorCaptcha').text("Please enter a value for Email Address");
            return false;
        } else if (!validateEmail(inputEmail)) {
            jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_lblErrorCaptcha').text("Please enter a valid Email Address");
                return false;
            }

        var txtArComments = jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_txtArComments').val();
        if (txtArComments == "") {
            jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_lblErrorCaptcha').text("Please enter a value for Comments");
            return false;
        }

        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_btnSubmitComments').hide();
        jgn("#imgAnimationSubmitComments").show();

        return true;
    }

    function cleanCaptcha() {
        Recaptcha.reload();
    }

    function onSuccessComments() {
        alert("Thank you for your comment. Once the administrator approves, the comment will be" +
                " shown on the site.");
        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_inputfirstName').val("");
        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_inputlastName').val("");
        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_inputEmail').val("");
        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_inputCompany').val("");
        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_inputFunction').val("");
        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_txtArComments').val("");
        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_lblErrorCaptcha').val("");
        cleanCaptcha();

        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_btnSubmitComments').show();
        jgn("#imgAnimationSubmitComments").hide();
    }

    function onCaptchaError() {
        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_lblErrorCaptcha').text("Incorrect captcha entry. Please refresh and try again");
        cleanCaptcha();

        jgn('#ctl00_SPWebPartManager1_g_53ff1dc6_e602_4006_989b_0563635783fe_ctl00_btnSubmitComments').show();
        jgn("#imgAnimationSubmitComments").hide();
    }

</script>


            if (url && url.trim().length > 0) {
                window.open(url);
                return false;
            }            
        });

        // Show URL on Mouse Hover
        jgn(".highlightBox").hover(function () {
            window.status = jgn(".hbContent").find("a:first").attr("href");
        }, function () {
            window.status = "";
        });

    });
</script>
<a id="ctl00_PlaceHolderMain_highlightBox_linkAnchor" class="linkAnchor">
</a>

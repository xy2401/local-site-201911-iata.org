    function ceoBlogCommentsValidate() {
            

        var firstName = jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_inputfirstName').val();
        if (firstName == "") {
            jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_lblErrorCaptcha').text("Please enter a value for First Name");
            return false;
        }

        var inputlastName = jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_inputlastName').val();
        if (inputlastName == "") {
            jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_lblErrorCaptcha').text("Please enter a value for Last Name");
            return false;
        }

        var inputEmail = jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_inputEmail').val();
        if (inputEmail == "") {
            jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_lblErrorCaptcha').text("Please enter a value for Email Address");
            return false;
        } else if (!validateEmail(inputEmail)) {
            jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_lblErrorCaptcha').text("Please enter a valid Email Address");
                return false;
            }

        var txtArComments = jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_txtArComments').val();
        if (txtArComments == "") {
            jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_lblErrorCaptcha').text("Please enter a value for Comments");
            return false;
        }

        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_btnSubmitComments').hide();
        jgn("#imgAnimationSubmitComments").show();

        return true;
    }

    function cleanCaptcha() {
        Recaptcha.reload();
    }

    function onSuccessComments() {
        alert("Thank you for your comment. Once the administrator approves, the comment will be" +
                " shown on the site.");
        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_inputfirstName').val("");
        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_inputlastName').val("");
        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_inputEmail').val("");
        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_inputCompany').val("");
        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_inputFunction').val("");
        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_txtArComments').val("");
        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_lblErrorCaptcha').val("");
        cleanCaptcha();

        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_btnSubmitComments').show();
        jgn("#imgAnimationSubmitComments").hide();
    }

    function onCaptchaError() {
        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_lblErrorCaptcha').text("Incorrect captcha entry. Please refresh and try again");
        cleanCaptcha();

        jgn('#ctl00_SPWebPartManager1_g_2a878577_96cb_4f27_b11f_9bfc6fe3bfa4_ctl00_btnSubmitComments').show();
        jgn("#imgAnimationSubmitComments").hide();
    }


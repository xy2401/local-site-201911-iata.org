    jgn('#SearchTextBox').on('click focusin', function() {
        if(this.value == 'Search on IATA website'){
            this.value = '';
        }    
    }).blur(function(){
        if(this.value.trim() == ''){
            this.value = 'Search on IATA website';
        }
    });


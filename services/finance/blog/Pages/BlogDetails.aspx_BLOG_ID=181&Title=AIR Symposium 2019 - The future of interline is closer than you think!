	honeypotCountries = ['CN','CN' ];
    jgn.ajax({
            type: 'GET',
            url: 'https://geoip-db.com/jsonp',
		    contentType: 'application/json',
		    jsonpCallback: 'callback',
            dataType: 'jsonp',
    		success: function(json){
        		console.log(json);
                if(jgn.inArray(json.country_code,honeypotCountries) == -1)
			        renderCaptcha();		
    		},
		    error: function(jqXHR, textStatus, errorThrown){
        		console.log(errorThrown);
                renderCaptcha();
    		}
    });

    function renderCaptcha() {
        console.log('recaptcha rendered');
		grecaptcha.render('IATArcfield', {'sitekey' : '6LdlD9YSAAAAACQBtcTazO6nEWLU5fESp492iGPR'});			
